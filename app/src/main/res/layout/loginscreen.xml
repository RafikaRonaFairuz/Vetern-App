<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context=".ui.loginScreen"
	android:background="#EFEFEF">

	<TextView
		android:id="@+id/slmt_dtg"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:fontFamily="@font/poppinssemibold"
		android:text="Hai,\nSelamat Datang\nKembali"
		android:textColor="#2D616C"
		android:textSize="34sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.206"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.057"
		tools:ignore="MissingConstraints" />

	<TextView
		android:id="@+id/email1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:fontFamily="@font/poppinssemibold"
		android:text="EMAIL"
		android:textColor="#2D616C"
		android:textSize="16sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.082"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/slmt_dtg"
		app:layout_constraintVertical_bias="0.094"
		tools:ignore="MissingConstraints" />

	<EditText
		android:id="@+id/email"
		android:layout_width="290dp"
		android:layout_height="50dp"
		android:background="@android:color/transparent"
		android:fontFamily="@font/poppinsregular"
		android:hint="Ketik di sini"
		android:inputType="textEmailAddress"
		android:paddingLeft="10dp"
		android:textColor="#2D616C"
		android:textColorHint="#CDCDCD"
		android:textSize="12sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.256"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/email1"
		app:layout_constraintVertical_bias="0.004"
		tools:ignore="MissingConstraints" />

	<RelativeLayout
		android:id="@+id/relativeLayout2"
		android:layout_width="320dp"
		android:layout_height="1dp"
		android:background="#65A896"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.436"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/email"
		app:layout_constraintVertical_bias="0.004"
		tools:ignore="MissingConstraints" />

	<TextView
		android:id="@+id/kata_sandi"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:fontFamily="@font/poppinssemibold"
		android:text="KATA SANDI"
		android:textColor="#2D616C"
		android:textSize="16sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.091"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/email"
		app:layout_constraintVertical_bias="0.135"
		tools:ignore="MissingConstraints" />

	<EditText
		android:id="@+id/password"
		android:layout_width="260dp"
		android:layout_height="50dp"
		android:background="@android:color/transparent"
		android:fontFamily="@font/poppinsregular"
		android:hint="Ketik di sini"
		android:inputType="textPassword"
		android:paddingLeft="10dp"
		android:textColor="#2D616C"
		android:textColorHint="#CDCDCD"
		android:textSize="12sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.256"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/kata_sandi"
		app:layout_constraintVertical_bias="0.0"
		tools:ignore="MissingConstraints" />

	<RelativeLayout
		android:id="@+id/relativeLayout"
		android:layout_width="320dp"
		android:layout_height="1dp"
		android:background="#65A896"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.436"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/password"
		app:layout_constraintVertical_bias="0.0"
		tools:ignore="MissingConstraints" />

	<Button
		android:id="@+id/tampilkan"
		android:layout_width="55dp"
		android:layout_height="12dp"
		android:background="@android:color/transparent"
		android:fontFamily="@font/poppinsregular"
		android:text="Tampilkan"
		android:textColor="#2D616C"
		android:textSize="10dp"
		app:layout_constraintBottom_toTopOf="@+id/relativeLayout"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.105"
		app:layout_constraintStart_toEndOf="@+id/password"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.962" />

	<Button
		android:id="@+id/lupa"
		android:layout_width="90dp"
		android:layout_height="12dp"
		android:background="@android:color/transparent"
		android:fontFamily="@font/poppinsregular"
		android:text="Lupa Kata Sandi?"
		android:textAllCaps="false"
		android:textColor="#802D616C"
		android:textSize="10dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.841"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/password"
		app:layout_constraintVertical_bias="0.05" />

	<Button
		android:id="@+id/login"
		android:layout_width="320dp"
		android:layout_height="40dp"
		android:background="@drawable/rectangle_btn_msk"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/lupa"
		app:layout_constraintVertical_bias="0.168"
		tools:ignore="MissingConstraints" />

	<View
		android:id="@+id/atau"
		android:layout_width="345dp"
		android:layout_height="26dp"
		android:background="@drawable/atau"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/login"
		app:layout_constraintVertical_bias="0.208"
		tools:ignore="MissingConstraints" />

	<TextView
		android:id="@+id/belum"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:fontFamily="@font/poppinsregular"
		android:text="Belum Memiliki Akun?"
		android:textColor="#2D616C"
		android:textAllCaps="false"
		android:textSize="10sp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.393"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/google"
		app:layout_constraintVertical_bias="0.311"
		tools:ignore="MissingConstraints" />

	<Button
		android:id="@+id/daftar"
		android:layout_width="65dp"
		android:layout_height="12dp"
		android:background="@android:color/transparent"
		android:fontFamily="@font/poppinsregular"
		android:text="Daftar Disini"
		android:textAllCaps="false"
		android:textColor="#802D616C"
		android:textSize="9dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.056"
		app:layout_constraintStart_toEndOf="@+id/belum"
		app:layout_constraintTop_toBottomOf="@+id/fb"
		app:layout_constraintVertical_bias="0.311" />

	<Button
		android:id="@+id/fb"
		android:layout_width="40dp"
		android:layout_height="40dp"
		android:background="@drawable/fb_logo"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.158"
		app:layout_constraintStart_toEndOf="@+id/google"
		app:layout_constraintTop_toBottomOf="@+id/atau"
		app:layout_constraintVertical_bias="0.257"
		tools:ignore="MissingConstraints" />

	<Button
		android:id="@+id/google"
		android:layout_width="40dp"
		android:layout_height="40dp"
		android:background="@drawable/google_logo"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.398"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/atau"
		app:layout_constraintVertical_bias="0.257"
		tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>